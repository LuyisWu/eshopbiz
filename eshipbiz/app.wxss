/**app.wxss**/
@import "/fonts/fonts.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
}
.text-center{
  text-align: center;
}
.relative{
  position: relative;
}
.absolute{
  position: absolute;
}
.msg-button{
  background: #505050;
  color: #fff;
  width: 190rpx;
  position:absolute;
  font-size:26rpx;
  top:18rpx;
  right: 40rpx;
  z-index: 9;
}
.c-state1{
  transform: rotate(0deg) scale(1) translate(75%,0%);   
  -webkit-transform: rotate(0deg) scale(1) translate(75%,0%);   
} 
